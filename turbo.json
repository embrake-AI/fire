{
	"$schema": "https://turbo.build/schema.json",
	"ui": "tui",
	"globalDependencies": [".env"],
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": [".output/**", "dist/**"],
			"inputs": ["$TURBO_DEFAULT$", ".env"]
		},
		"type-check": {},
		"lint": {},
		"dev": {
			"cache": false,
			"persistent": true
		},
		"clean": {
			"cache": false
		}
	}
}
